<html>
    <head>
        <title>View Items</title>
    </head>
<body>
<h1>View Items</h1>
<table border=1>
    <tr>
        <td>Brand</td>
        <td>Name</td>
        <td>Color</td>
        <td>Price</td>
        <td>Quantity</td>
        <td></td>
    </tr>
<% ditems.forEach(function(ditems){ %>
    <tr>
        <td><%=ditems.ibrand %></td>
        <td><%=ditems.iname %></td>
        <td><%=ditems.icolor %></td>
        <td><%=ditems.iprice %></td>
        <td><%=ditems.iqty %></td>
        <td><a href="items/<%=ditems._id %>/edit">Edit Item</a></td>
        <td><a href="items/<%=ditems._id %>/destroy">Delete Item</a></td>

    </tr>
<% }) %>
</table>
</body>
</html>